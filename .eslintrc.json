{
    "env": {
        "browser": true,
        "es2021": true,
        "eidtor.formatOnSave": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars":"off",
        "prefer-const":"off",
        "prettier/prettier": ["error", {
            "endOfLine": "auto"
        }],
        "func-names":"off",
        "no-console":"warn",
        "no-use-before-define":"warn",
        "no-param-reassign":"off",
        "object-shorthand":"off",
        "prefer-destructuring":"off",
        "no-shadow":"off"
    }
}